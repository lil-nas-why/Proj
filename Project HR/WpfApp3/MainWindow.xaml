<Window x:Class="WpfApp3.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WpfApp3"
        mc:Ignorable="d"
        Title= "Elbrus-Tech" Height="522" Width="826" MinHeight="522" MinWidth="826" FontFamily=" Century Gothic" WindowStartupLocation="CenterScreen">
    <Grid>
        <Border Background="#1E90FF"/>
        <Border Background="#1E90FF" Grid.Row="2"/>

        <Frame Grid.Row="1" Name="MainFrame" Width="NaN" Height="NaN" NavigationUIVisibility="Hidden" ContentRendered="MainFrame_ContentRendered" />
        <Button Name="GoBackButton" Content=" Вернуться назад " Grid.Row="2" Margin="7" HorizontalAlignment="Right" Visibility="Hidden"
                Click="GoBackButton_Click"/>

        <Grid.RowDefinitions>
            <RowDefinition Height="50"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="50"/>
        </Grid.RowDefinitions>

        



    </Grid>


</Window>
